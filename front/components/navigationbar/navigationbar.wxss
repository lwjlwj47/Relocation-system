/* components/navigationbar/navigationbar.wxss */.navigation {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5001;
  width: 100%;
  color: #fff;
  display: flex;
  align-items: center;
}
 
.navigation_bar_left {
  padding-left: 10rpx;
  align-items: center;
}
 
.navigation_bar_left .icon-fanhuijian{
  font-size: 30rpx;
}
 
.navigation_bar_center {
  font-size: 26rpx;
  text-align: center;
  position: absolute;
  left: 50%;
  transform: translate(-50%, 0);
  flex: 1;
}